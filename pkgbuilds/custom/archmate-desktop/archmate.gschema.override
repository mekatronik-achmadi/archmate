################################# PANEL ########################################

[org.mate.NotificationDaemon]
popup-location='bottom_right'
theme='standard'

[org.mate.panel.applet.workspace-switcher]
num-rows=1

[org.mate.panel.objects.workspace-switcher.prefs]
num-rows=1

[org.mate.Marco.general]
num-workspaces=2

[org.mate.panel.applet.clock]
show-date=true

[org.mate.panel]
default-layout='fedora'

[org.mate.panel.menubar]
show-icon=true
max-recent-items=8
max-items-or-submenu=8

[org.mate.caja.desktop]
home-icon-visible=true
trash-icon-visible=true
computer-icon-visible=true
volumes-visible=false
network-icon-visible=false

################################# APP ##########################################

[org.mate.applications-browser]
exec='vimb'
nremote=false
needs-term=false

[org.mate.applications-terminal]
exec='mate-terminal'
exec-arg='-x'

[org.mate.caja.preferences]
default-sort-in-reverse-order=false
default-sort-order='type'
show-directory-item-counts='always'
show-icon-text='never'
show-image-thumbnails='always'
thumbnail-limit=1048576
preview-sound='never'
default-folder-viewer='compact-view'
exit-with-last-window=false
enable-delete=true
confirm-move-to-trash=true
show-backup-files=true
show-advanced-permissions=true

[org.mate.caja.compact-view]
default-zoom-level='large'

[org.mate.caja.window-state]
start-with-status-bar=true
start-with-toolbar=true
start-with-sidebar=true

[org.mate.caja-open-terminal]
desktop-opens-home-dir=false

[org.mate.terminal.global]
use-mnemonics=false

[org.mate.calc]
button-mode='programming'
show-thousands=true

[org.mate.engrampa.dialogs.batch-add]
default-extension='.tar.gz'

[org.mate.eom.ui]
sidebar=false
toolbar=false

[org.mate.eom.plugins]
active-plugins=['reload', 'statusbar-date']

[org.mate.pluma]
auto-save=true
auto-save-interval=10
bracket-matching=true
toolbar-visible=false
statusbar-visible=true
side-pane-visible=false
display-line-numbers=true
display-right-margin=true
bottom-panel-visible=false
highlight-current-line=false
search-highlighting=true
syntax-highlighting=true
tabs-size=4
auto-indent=true
insert-spaces=true
wrap-mode='GTK_WRAP_NONE'
active-plugins=['externaltools', 'sort', 'trailsave', 'quickopen', 'docinfo', 'time', 'snippets', 'changecase', 'filebrowser', 'modelines']

[org.mate.terminal.global]
use-mnemonics=false
use-menu-accelerators=false

[org.mate.terminal.profile]
use-custom-default-size=true
default-show-menubar=false
default-size-columns=80
default-size-rows=25
cursor-blink-mode='off'

[org.gnome.meld]
highlight-syntax=true
show-line-numbers=true
highlight-current-line=false

################################# MATE #########################################

[org.mate.interface]
toolbar-icons-size='small-toolbar'
toolbar-style='icons'
automatic-mnemonics=false
enable-animations=false

[org.mate.media-handling]
automount-open=false
autorun-never=true
automount=false

[org.mate.Marco.general]
compositing-manager=false
center-new-windows=false
focus-mode='click'

[org.mate.Marco.global-keybindings]
run-command-terminal='<Control><Alt>t'
panel-run-dialog='<Control><Alt>r'
panel-main-menu='Super_L'
switch-panels='disabled'
switch-windows='<Alt>Tab'
switch-windows-all='<Control><Alt>Tab'
switch-windows-all-backward='<Control><Alt><Shift>Tab'
switch-windows-backward='<Alt><Shift>Tab'

[org.mate.Marco.window-keybindings]
tile-to-side-e='<Primary><Shift>Right'
tile-to-side-w='<Primary><Shift>Left'
tile-to-corner-ne='<Primary><Shift>u'
tile-to-corner-nw='<Primary><Shift>y'
tile-to-corner-se='<Primary><Shift>j'
tile-to-corner-sw='<Primary><Shift>h'

[org.mate.SettingsDaemon.plugins.media-keys]
control-center='<Control><Alt>q'
search='<Primary><Alt>f'
www='<Control><Alt>w'
home='<Control><Alt>e'
power='disabled'
screensaver='disabled'

[org.mate.peripherals-touchpad]
disable-while-typing=true
tap-to-click=true

[org.mate.peripherals-keyboard-xkb.kbd]
layouts=['us']

[org.mate.power-manager]
action-critical-battery='suspend'
button-lid-ac='suspend'
button-lid-battery='suspend'
button-power='interactive'
button-suspend='suspend'
icon-policy='present'
idle-dim-ac=true
sleep-computer-ac=1800
sleep-computer-battery=1800
sleep-display-ac=600

[org.mate.screensaver]
mode='single'
themes=['screensavers-personal-slideshow']
idle-activation-enabled=false
lock-enabled=false

[org.mate.sound]
event-sounds=true
theme-name='freedesktop'
input-feedback-sounds=true

[org.mate.background]
show-desktop-icons=true

[org.mate.session]
required-components-list=['windowmanager', 'filemanager', 'panel']

[org.mate.session.required-components]
windowmanager='marco'
filemanager='caja'
panel='mate-panel'
dock=''

################################# THEME ########################################

[org.gnome.desktop.interface]
gtk-theme='Arc-Lighter-solid'
icon-theme='Papirus-Light'
gtk-color-scheme=''

[org.gnome.desktop.wm.preferences]
theme='Arc-Lighter-solid'

[org.gnome.desktop.interface]
cursor-size=24
cursor-theme='mate-black'

[org.mate.interface]
gtk-theme='Arc-Lighter-solid'
icon-theme='Papirus-Light'
gtk-color-scheme=''

[org.mate.file-views]
icon-theme='Papirus-Light'

[org.mate.Marco.general]
theme='Arc-Lighter-solid'

[org.mate.peripherals-mouse]
cursor-size=24
cursor-theme='mate-black'

[org.mate.peripherals-keyboard-xkb.indicator]
foreground-color='0 0 0'

[org.mate.background]
picture-filename='/usr/share/backgrounds/archlinux/conference.png'
picture-options='stretched'

[org.mate.pluma]
color-scheme='kate'

[org.mate.terminal.profile]
use-theme-colors=false
palette='#000000000000:#CCCC00000000:#4E4D9A9A0605:#C4C3A0A00000:#34346564A4A3:#7575504F7B7B:#060598979A9A:#D3D3D7D6CFCF:#555457565352:#EFEF29282928:#8A89E2E23434:#FCFBE9E84F4F:#72729F9ECFCF:#ADAC7F7EA8A8:#3434E2E2E2E2:#EEEDEEEDECEB'
background-color='#FFFFFF'
foreground-color='#000000'
bold-color='#000000'

[org.gnome.meld]
style-scheme='kate'

[org.yorba.shotwell.preferences.ui]
use-dark-theme=false

[org.onboard]
layout='/usr/share/onboard/layouts/Compact.onboard'
theme='/usr/share/onboard/themes/Classic Onboard.theme'

